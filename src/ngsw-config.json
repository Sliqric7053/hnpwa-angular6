{
  "index": "/index.html",
  "assetGroups": [
    {
      "name": "app",
      "installMode": "prefetch",
      "resources": {
        "files": ["/index.html", "/*.bundle.css", "/*.bundle.js", "/*.chunk.js"]
      }
    },
    {
      "name": "assets",
      "installMode": "lazy",
      "updateMode": "prefetch",
      "resources": {
        "files": ["/assets/**"]
      }
    }
  ],
  "dataGroups": [
    {
      "name": "api-performance",
      "urls": [
        "/",
        "/hn",
        "/news",
        "/top",
        "/new",
        "/ask",
        "/show",
        "/jobs",
        "/item",
        "/user"
      ],
      "cacheConfig": {
        "maxSize": 100000,
        "maxAge": "7d",
        "strategy": "performance"
      }
    }
  ]
}
